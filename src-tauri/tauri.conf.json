{
  "productName": "n-nacos",
  "version": "1.0.0",
  "identifier": "n-nacos",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:9000",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../../n-nacos-web/dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "n-nacos",
        "fullscreen": false,
        "resizable": true,
        "minWidth": 1024,
        "minHeight": 960,
        "width": 1024,
        "height": 960
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "publisher": "poohlaha",
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": ["resources/*", ".env"],
    "macOS": {
      "entitlements": "entitlements.plist",
      "exceptionDomain": ""
    }
  }
}
